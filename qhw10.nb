Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"The", " ", "n"}], "=", 
    RowBox[{
    "1", " ", "wavefunction", " ", "for", " ", "our", " ", "unperturbed", " ",
      "case"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"\[Psi]o1", "[", "x_", "]"}], ":=", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       FractionBox["2", "L"], "]"}], "*", 
      RowBox[{"Sin", "[", 
       FractionBox[
        RowBox[{"\[Pi]", " ", "x"}], "L"], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "We", " ", "use", " ", "the", " ", 
     "\"\<most important formula in quantum physics\n to calculate the 1st \
order correction to the energy \>"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"E11", 
    RowBox[{"(*", " ", 
     RowBox[{"=", 
      RowBox[{"{", 
       RowBox[{"\[Psi]o1", "|", 
        RowBox[{"H", "'"}], "|", "\[Psi]o1"}], "}"}]}], " ", "*)"}], "=", 
    RowBox[{
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{"V0", "*", 
        RowBox[{
         RowBox[{"\[Psi]o1", "[", "x", "]"}], "^", "2"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", 
         FractionBox[
          RowBox[{"3", "L"}], "8"], ",", 
         FractionBox[
          RowBox[{"5", "L"}], "8"]}], "}"}]}], "]"}], "=", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", 
          SqrtBox["2"]}], "+", "\[Pi]"}], ")"}], " ", "V0"}], 
      RowBox[{"4", " ", "\[Pi]"}]]}]}], " ", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
     "We", " ", "repeat", " ", "the", " ", "same", " ", "exact", " ", 
      "process", " ", "for", " ", "the", " ", "n"}], "=", 
     RowBox[{"2", " ", "wavefunction"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Psi]o2", "[", "x_", "]"}], ":=", 
     RowBox[{
      RowBox[{"Sqrt", "[", 
       FractionBox["2", "L"], "]"}], "*", 
      RowBox[{"Sin", "[", 
       FractionBox[
        RowBox[{"2", "\[Pi]", " ", "x"}], "L"], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"E12", 
     RowBox[{"(*", " ", 
      RowBox[{"=", 
       RowBox[{"{", 
        RowBox[{"\[Psi]o1", "|", 
         RowBox[{"H", "'"}], "|", "\[Psi]o1"}], "}"}]}], " ", "*)"}], "=", 
     RowBox[{
      RowBox[{"Integrate", "[", 
       RowBox[{
        RowBox[{"V0", "*", 
         RowBox[{
          RowBox[{"\[Psi]o2", "[", "x", "]"}], "^", "2"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", 
          FractionBox[
           RowBox[{"3", "L"}], "8"], ",", 
          FractionBox[
           RowBox[{"5", "L"}], "8"]}], "}"}]}], "]"}], "=", 
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "2"}], "+", "\[Pi]"}], ")"}], " ", "V0"}], 
       RowBox[{"4", " ", "\[Pi]"}]]}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<E11= \>\"", ",", "E11", ",", "\"\<, and E22= \>\"", ",", "E12", ",", 
     "\"\<. Where E1, E2 are the 1st order corrections to the n=1 and n=2 \
states of the infinite square well problem .\>\""}], "]"}], 
   "\[IndentingNewLine]"}]}]], "Input",
